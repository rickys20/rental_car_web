<body>  
    <div class="content">
      <div class="sidebar">
        <div class="fix-sidebar">
          <div class="bag-1">
            <img src="./img/Rectangle 63logo.png" style="margin-left: 25%; margin-top: 30%;">
            <img src="./img/Home.png" style="margin-left: 5%; margin-top: 30%;">
            <img src="./img/Administrator.png" style="margin-left: 5%; margin-top: 30%;">
          </div>
          <div class="bag-2">
            <div class="nav-side bg-light">
              <img src="./img/Rectangle 62.png" style="margin-left: 10%; margin-top: 8%;">
            </div>
            <div class="menu-side">
              <button type="button" class="menu-list w-100"><a href="#">Cars</a></button>
              <button type="button" class="menu-list w-100"><a href="/">List Car</a></button>
            </div>
          </div>
        </div>
      </div>
      <div class="main-content" style="background-color:#E5E5E5">
